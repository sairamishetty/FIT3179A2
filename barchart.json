{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Energy Usuage Across Australia.",
  "data": {"url": "https://raw.githubusercontent.com/sairamishetty/FIT3179A2/main/data.csv"},

  "mark": "line",
  "encoding": {
    "x": {"field": "Date", "type": "temporal"},
    "y": {"field": "Energy", "type": "quantitative"},
    "color": {"field": "EnergyType", "type": "nominal"}
  },
  "transform": [
    {"fold": [
      "Energy from natural inputs - brown coal",
      "Energy from natural inputs - black coal",
      "Energy from natural inputs - natural gas",
      "Energy from natural inputs - crude oil and condensates"
    ], "as": ["EnergyType", "Energy"]}
  ]
}